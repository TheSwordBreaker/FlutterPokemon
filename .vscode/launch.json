{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Web_auth_example",
			"request": "launch",
			"type": "dart",

			"args": ["run -d chrome --web-port=54268 --web-renderer=html"]
		},
		{
			"name": "Attach to Chrome",
			"port": 9222,
			"request": "attach",
			"type": "chrome",
			"webRoot": "${workspaceFolder}"
		},
		{
			"name": "auth_example",
			"request": "launch",
			"type": "dart"
		},
		{
			"name": "auth_example (profile mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile"
		},
		{
			"name": "auth_example (release mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release"
		}
	]
}
